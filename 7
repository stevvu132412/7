getgenv().autostrength = false
getgenv().autowins = false
getgenv().autorebirth = false
getgenv().autoteleport = false

local ReplicatedStorage = game:GetService("ReplicatedStorage")


local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/bloodball/-back-ups-for-libs/main/kav"))()
local Window = Library.CreateLib("Power Slapping Simulator | AzuHub", "DarkTheme")
local Tab = Window:NewTab("Main")
local Section1 = Tab:NewSection("Main")
local Tab = Window:NewTab("Teleport")
local Section3 = Tab:NewSection("Boss Maps")
local Section2 = Tab:NewSection("Teleports")
local Tab = Window:NewTab("Misc")
local Section4 = Tab:NewSection("Player")
local Tab = Window:NewTab("Credits")
local Section100 = Tab:NewSection("Made By azuress")
local Section101 = Tab:NewSection("discord.gg/htKA3GkPay")




Section1:NewToggle("Auto Click", "Auto Clicks For You", function(state)
    if state then
        getgenv().autostrength = true
        while getgenv().autostrength do
            task.wait()
            local args = {[1] = "ClickStat",[2] = 9999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999}
            ReplicatedStorage.Remotes.Client:InvokeServer(unpack(args))
        end
    else
        getgenv().autostrength = false
    end
end)

Section1:NewToggle("Auto Wins", "Auto Wins For You", function(state)
    if state then
        getgenv().autowins = true
        while getgenv().autowins do
            task.wait()
            local args = {[1] = "Wins"}
            ReplicatedStorage.Remotes.Client:InvokeServer(unpack(args))
        end
    else
        getgenv().autowins = false
    end
end)

Section1:NewToggle("Auto Rebirth", "Auto Rebirths For You", function(state)
    if state then
        getgenv().autorebirth = true
        while getgenv().autorebirth do
            task.wait()
            ReplicatedStorage.Remotes.Rebirth:FireServer()
        end
    else
        getgenv().autorebirth = false
    end
end)

Section1:NewButton("Redeem All Codes", "Redeems All Codes", function()
    local args = {[1] = "UPDATE"}
    game:GetService("ReplicatedStorage").CodesRemote:InvokeServer(unpack(args))
end)

function teleportTO(placeCFrame)
    local plyr = game.Players.LocalPlayer
    if plyr.Character then
        plyr.Character.HumanoidRootPart.CFrame = placeCFrame
    end
end

Section2:NewButton("Spawn", "Telepoorts You To Spawn", function()
    teleportTO(game:GetService("Workspace").SpawnLocation.CFrame)
end)

