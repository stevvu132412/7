Section4:NewToggle("WalkSpeed", "Changes Your WalkSpeed", function(state)
    if state then
        getgenv().autowalk = true
        while getgenv().autowalk do
            task.wait()
            game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 100
        end
    else
        game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 26
        getgenv().autowalk = false
    end
end)

Section4:NewButton("AntiAfk", "Afks For You", function()
    wait(3)
    local VirtualUser=game:service'VirtualUser'
    game:service('Players').LocalPlayer.Idled:connect(function()
    VirtualUser:CaptureController()
    VirtualUser:ClickButton2(Vector2.new())
    end)
end)



